
public class TestaVariaveis {

	public static void main(String[] args) {
		System.out.println("ola novo teste");
		
//variaveis do tipo inteiro
		
		int idade;
		idade = 31;
		
		System.out.println(idade);
		
		idade = 30 + 1;
		idade = (6*5) + 1;
		
		System.out.println("A idade é " + idade + " parabens");
		
	}
}
