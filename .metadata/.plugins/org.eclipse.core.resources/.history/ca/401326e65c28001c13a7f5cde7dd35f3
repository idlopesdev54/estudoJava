
public class Exercicios {

	public static void main(String[] args) {
		int dia = 4;
		int outroDia = 4 + dia;
		System.out.println(outroDia);
		
		int brindes = 15;
		int pessoas = 2;
		System.out.println(brindes / pessoas);
		
		double peso = 4.0;
		int quantidade = 10;
		System.out.println(peso * quantidade);
		
		double peso1 = 4.0;
		int quantidade1 = 10;
		System.out.println(peso1*quantidade1);
		
	}
 }
